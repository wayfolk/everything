
{
  "name": "@wayfolk/everything",
  "dependencies":
  {
  },
  "devDependencies":
  {
    "npm-check-updates": "16.14.4",
    "concurrently": "8.2.1"
  },
  "scripts":
  {
    "common-development-web-builder_install": "cd ./common/_development/_web/_builder/ && npm install",
    "common-development-web-builder_update": "cd ./common/_development/_web/_builder/ && ./node_modules/.bin/ncu",
    "common-development-web-builder_upgrade": "cd ./common/_development/_web/_builder/ && ./node_modules/.bin/ncu --upgrade && npm install",
    "common-development-web-builder_clean": "cd ./common/_development/_web/_builder/ && rm -rf ./node_modules/ && rm -f ./package-lock.json",


    "common-development-web-server_install": "cd ./common/_development/_web/_server/ && npm install",
    "common-development-web-server_update": "cd ./common/_development/_web/_server/ && ./node_modules/.bin/ncu",
    "common-development-web-server_upgrade": "cd ./common/_development/_web/_server/ && ./node_modules/.bin/ncu --upgrade && npm install",
    "common-development-web-server_clean": "cd ./common/_development/_web/_server/ && rm -rf ./node_modules/ && rm -f ./package-lock.json",
    "common-development-web-server_start": "cd ./common/_development/_web/_server/ && node Server.mjs",


    "common-development-web-engine_install": "cd ./common/_development/_web/_engine/ && npm install",
    "common-development-web-engine_update": "cd ./common/_development/_web/_engine/ && ./node_modules/.bin/ncu",
    "common-development-web-engine_upgrade": "cd ./common/_development/_web/_engine/ && ./node_modules/.bin/ncu --upgrade && npm install",
    "common-development-web-engine_clean": "cd ./common/_development/_web/_engine/ && rm -rf ./node_modules/ && rm -f ./package-lock.json",
    "common-development-web-engine_build": "cd ./common/_development/_web/_builder/ && node Builder.mjs --project $INIT_CWD/common/_development/_web/_engine/",
    "common-development-web-engine_build-watch": "cd ./common/_development/_web/_builder/ && node Builder.mjs --project $INIT_CWD/common/_development/_web/_engine/ --watch",


    "common-development-runtimes-node-chromium_install": "cd ./common/_development/_runtimes/_node-chromium/ && npm install",
    "common-development-runtimes-node-chromium_update": "cd ./common/_development/_runtimes/_node-chromium/ && ./node_modules/.bin/ncu",
    "common-development-runtimes-node-chromium_upgrade": "cd ./common/_development/_runtimes/_node-chromium/ && ./node_modules/.bin/ncu --upgrade && npm install",
    "common-development-runtimes-node-chromium_clean": "cd ./common/_development/_runtimes/_node-chromium/ && rm -rf ./node_modules/ && rm -f ./package-lock.json",
    "common-development-runtimes-node-chromium_dev": "cd ./common/_development/_runtimes/_node-chromium/ && node ./_electron/dev.mjs",
    "common-development-runtimes-node-chromium_build": "cd ./common/_development/_runtimes/_node-chromium/ && node ./_electron/build.mjs",


    "projects-wayf0000-install": "cd ./wayf0000/_development/_web/_codebase/_projects/wayf0000/ && npm install",
    "projects-wayf0000-update": "cd ./wayf0000/_development/_web/_codebase/_projects/wayf0000/ && ./node_modules/.bin/ncu",
    "projects-wayf0000-upgrade": "cd ./wayf0000/_development/_web/_codebase/_projects/wayf0000/ && ./node_modules/.bin/ncu --upgrade && npm install",
    "projects-wayf0000-clean": "cd ./wayf0000/_development/_web/_codebase/_projects/wayf0000/ && rm -rf ./node_modules/ && rm -f ./package-lock.json && rm -rf ./_build/",
    "projects-wayf0000-build": "cd ./common/_development/_web/_builder/ && node Builder.mjs --project $INIT_CWD/wayf0000/_development/_web/_codebase/_projects/wayf0000/",
    "projects-wayf0000-build-watch": "cd ./common/_development/_web/_builder/ && node Builder.mjs --project $INIT_CWD/wayf0000/_development/_web/_codebase/_projects/wayf0000/ --watch",


    "projects-theu0000-install": "cd ./wayf0000/_development/_web/_codebase/_projects/theu0000/ && npm install",
    "projects-theu0000-update": "cd ./wayf0000/_development/_web/_codebase/_projects/theu0000/ && ./node_modules/.bin/ncu",
    "projects-theu0000-upgrade": "cd ./wayf0000/_development/_web/_codebase/_projects/theu0000/ && ./node_modules/.bin/ncu --upgrade && npm install",
    "projects-theu0000-clean": "cd ./wayf0000/_development/_web/_codebase/_projects/theu0000/ && rm -rf ./node_modules/ && rm -f ./package-lock.json && rm -rf ./_build/",
    "projects-theu0000-build": "cd ./common/_development/_web/_builder/ && node Builder.mjs --project $INIT_CWD/wayf0000/_development/_web/_codebase/_projects/theu0000/",
    "projects-theu0000-build-watch": "cd ./common/_development/_web/_builder/ && node Builder.mjs --project $INIT_CWD/wayf0000/_development/_web/_codebase/_projects/theu0000/ --watch",


    "wayf0000-install": "concurrently -p \"{name}:\" -c \"dim\" \"npm:common-development-web-builder_install\" \"npm:common-development-web-server_install\" \"npm:projects-theu0000-install\"",
    "wayf0000-dev": "",


    "theu000-install": "concurrently -p \"{name}:\" -c \"dim\" \"npm:common-development-web-builder_install\" \"npm:common-development-web-server_install\" \"npm:projects-wayf0000-install\"",
    "theu000-dev": "concurrently -p \"{name}:\" -n \"server_start,theu0000-build-watch\" -c \"dim\" \"npm:common-development-web-server_start\" \"npm:projects-theu0000-build-watch\"",


    "engine-install": "concurrently -p \"{name}:\" -c \"dim\" \"npm:common-development-web-builder_install\" \"npm:common-development-web-server_install\" \"npm:common-development-web-engine_install\"",
    "engine-dev": "concurrently -p \"{name}:\" -n \"server_start,engine_build-watch\" -c \"dim\" \"npm:common-development-web-server_start\" \"npm:common-development-web-engine_build-watch\"",


    "runtime-install": "concurrently -p \"{name}:\" -c \"dim\" \"npm:common-development-runtimes-node-chromium_install\"",
    "runtime-dev": "concurrently -p \"{name}:\" -n \"node-chromium_dev\" -c \"dim\" \"npm:common-development-runtimes-node-chromium_dev\"",


    "update": "./node_modules/.bin/ncu",
    "upgrade": "npm install",
    "clean": "rm -rf ./node_modules/ && rm -f ./package-lock.json"
  },
  "os":
  [
    "linux"
  ],
  "cpu":
  [
    "x64"
  ],
  "private": true
}